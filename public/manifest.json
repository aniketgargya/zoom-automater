{
    "name": "Zoom Automater",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Zoom Automater"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "128": "zoom.png"
    }
}