{
    "name": "Zoom Automation",
    "version": "1.1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "Zoom"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["storage", "tabs"],
    "icons": {
        "128": "icons/zoom.png"
    }
}
